{
  "version": 2,
  "rewrites": [
    { "source": "/static/:path*", "destination": "/api/static" }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x",
      "memory": 3008,
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ]
}
